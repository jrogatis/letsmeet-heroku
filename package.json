{
  "name": "letsmeet",
  "version": "0.0.3",
  "description": "Schedule events with ease!",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build:server": "babel -d ./build ./server -s",
    "build:client": "webpack -p  --config ./webpack.production.config.js",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "start": "node -r dotenv/config ./build/app.js",
    "serve": "npm run clean && npm run build:server && npm run start",
    "postinstall": "npm run build",
    "deploy": "git push heroku development:master"
  },
